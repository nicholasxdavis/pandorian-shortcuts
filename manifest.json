{
  "manifest_version": 3,
  "name": "Pandorian",
  "version": "1.1.0",
  "description": "The ultimate command bar for your browser. Use @tags to navigate instantly.",
  "permissions": [
    "storage",
    "tabs",
    "webNavigation"
  ],
  "host_permissions": [
    "*://www.google.com/*",
    "*://www.google.*/*",
    "*://www.bing.com/*",
    "*://duckduckgo.com/*",
    "*://search.yahoo.com/*",
    "*://www.ecosia.org/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "img/logo.png",
      "48": "img/logo.png",
      "128": "img/logo.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  }
}